<td colspan="8" class="text-center">
    <ul class="pagination">
        <li {{#if pagination.isFirstPage}}class="disabled"{{/if}}>
        <span class="first"><i class="glyphicon glyphicon-fast-backward"></i><span class="sr-only">First</span></span>
        </li>
        <li {{#if pagination.isFirstPage}}class="disabled"{{/if}}>
        <span class="prev"><i class="glyphicon glyphicon-chevron-left"></i><span class="sr-only">Previous</span></span>
        </li>
        {{#paginate pagination type="middle" limit="7"}}
        <li {{#if active}}class="active"{{/if}}><a href="/#tickets/list/{{n}}">{{n}}{{#if active}}<span class="sr-only">(current)</span>{{/if}}</a></li>
        {{/paginate}}
        <li {{#if pagination.isLastPage}}class="disabled"{{/if}}>
                <span class="next"><i class="glyphicon glyphicon-chevron-right"></i><span
                        class="sr-only">Next</span></span>
        </li>
        <li {{#if pagination.isLastPage}}class="disabled"{{/if}}>
                <span class="last"><i class="glyphicon glyphicon-fast-forward"></i><span
                        class="sr-only">Last</span></span>
        </li>
    </ul>
    <ul class="pagination pull-right">
        <li>
            Page <b class="current">{{currentPage}}</b> of <b class="total">{{totalPages}}</b>
        </li>
    </ul>
</td>